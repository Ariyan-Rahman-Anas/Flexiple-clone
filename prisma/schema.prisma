generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

type DeveloperExperiences {
  companyName String
  duration    Int
  endDate     String
  role        String
  startDate   String
}

type DeveloperUserSkills {
  skill DeveloperUserSkillsSkill
}

type DeveloperUserSkillsSkill {
  name String
}

type DevelopersExperiences {
  companyName String
  duration    Int
  endDate     String
  role        String
  startDate   String
}

type DevelopersUserSkills {
  skill DevelopersUserSkillsSkill
}

type DevelopersUserSkillsSkill {
  name String
}

model developer {
  id           String                 @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    String
  currentCTC   String
  experience   Int
  experiences  DeveloperExperiences[]
  name         String
  noticePeriod String
  updatedAt    String
  userSkills   DeveloperUserSkills[]
}

model developers {
  id           String                  @id @default(auto()) @map("_id") @db.ObjectId
  createdAt    String
  currentCTC   String
  experience   Int
  experiences  DevelopersExperiences[]
  name         String
  noticePeriod String
  updatedAt    String
  userSkills   DevelopersUserSkills[]
}
